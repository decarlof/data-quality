# .coveragerc to control coverage.py
[run]
source=dquality
[report]
omit =
    */python?.?/*
    */site-packages/nose/*
    *realtime/*
    *test*
    *__init__.py*
    *common.__init__.py*
    *common.constants.py*
    *setup.py*
   
exclude_lines =
    def set_default

[html]
directory = coverage_html_report
